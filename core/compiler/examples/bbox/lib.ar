cell bot() {
    let met1 = rect("met1", x0=0., y0=0., x1=100., y1=100.)!;
}

cell top() {
    let bot_cell = bot();
    let r = bbox(bot_cell);
    rect("met2", x0=r.x0, x1=r.x1, y0=r.y0, y1=r.y1)!;
    let inst0 = inst(bot_cell)!;
    eq(inst0.x, 100.);
    eq(inst0.y, 100.);
    let r2 = bbox(inst0);
    rect("met3", x0=r2.x0, x1=r2.x1, y0=r2.y0, y1=r2.y1)!;
}
